<%= form_for([@skateboarder.skatepark, @skateboarder]) do |f| %>
<%= f.hidden_field :skatepark_id, value: @skateboarder.skatepark.id %>

<%= f.label :name, "Name" %><br>
<%= f.text_field :name %><br>

<%= f.label :experience, "Experience" %><br>
<%= f.text_field :experience %><br>

<%= f.label :favorite_spots, "Favorite Spots" %><br>
<%= f.text_field :favorite_spots %><br>

<%= f.submit %><br>

<% end %>
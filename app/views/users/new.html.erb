<h1>Welcome to Antibi, Please Apply Here</h1>

<%if @error %>
  <% @error.each do |e|%>
   <h1> <% e %> </h1>
   <%end %>
<%end %>

<%= form_for @user do |f| %>
  <div>
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <%= f.submit "Signup" %>
<% end %>